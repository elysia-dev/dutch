    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@types/react-native-dotenv": "^0.2.0",
    "axios": ">=0.21.1",
    "base-64": "^1.0.0",
    "bignumber.js": "^9.0.1",
    "crypto-es": "^1.2.7",
    "ethers": "^5.0.24",
    "expo": "^41.0.0",
    "expo-barcode-scanner": "~10.1.2",
    "expo-clipboard": "~1.0.2",
    "expo-constants": "~10.1.3",
    "expo-file-system": "~11.0.2",
    "expo-font": "~9.1.0",
