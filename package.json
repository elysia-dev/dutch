    "lint-staged": "^10.4.0",
    "react-test-renderer": "~16.11.0",
    "typescript": "~4.0.0"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit --no-verify": [
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{md,mdx,json,css,scss,yaml,yml}": [
      "prettier --write"
    ]
  },
  "name": "dutch",
  "version": "1.1.0"
